<data-extraction-rules>
    <!-- Use default backup files except the lldb debugger directory -->
    <cloud-backup>
        <exclude domain="root" path="lldb"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="root" path="lldb"/>
    </device-transfer>
</data-extraction-rules>
